            <discovery_rules>
                <discovery_rule>
                    <name>JunOS Interfaces</name>
                    <type>SNMPV3</type>
                    <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.2.1.2.2.1.2]</snmp_oid>
                    <key>discovery.ifDescr</key>
                    <delay>300</delay>
                    <snmpv3_securityname>{$JUN_V3_USER}</snmpv3_securityname>
                    <snmpv3_securitylevel>AUTHPRIV</snmpv3_securitylevel>
                    <snmpv3_authprotocol>SHA</snmpv3_authprotocol>
                    <snmpv3_authpassphrase>{$JUN_V3_AUTHPASS}</snmpv3_authpassphrase>
                    <snmpv3_privprotocol>AES</snmpv3_privprotocol>
                    <snmpv3_privpassphrase>{$JUN_V3_AUTHPASS}</snmpv3_privpassphrase>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#SNMPVALUE}</macro>
